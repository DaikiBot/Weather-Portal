{
    "functions": {
        "api/python/index.py": {
            "runtime": "python3.9"
        },
        "index.js": {
            "runtime": "nodejs14.x"
        }
    },
    "rewrites": [
        {
            "source": "/get-lightning-data",
            "destination": "/api/python/index.py"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}