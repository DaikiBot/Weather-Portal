{
    "functions": {
        "api/python/index.py": {
            "runtime": "vercel-python@0.5.2"
        },
        "index.js": {
            "runtime": "vercel-node@2.0.0"
        }
    },
    "rewrites": [
        {
            "source": "/get-lightning-data",
            "destination": "/api/python/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.js"
        }
    ]
}